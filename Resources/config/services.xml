<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="shivas.pheal.factory.class">shivas\PhealBundle\Service\PhealFactory</parameter>
    </parameters>

    <services>
        <service id="shivas.pheal.factory" class="%shivas.pheal.factory.class%">
            <argument>%shivas.pheal.config%</argument>
        </service>
    </services>
</container>
